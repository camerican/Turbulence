        <h1 align="center">Stream Board</h1>
      <div class="stream_board">

      <iframe
    src="http://player.twitch.tv/?channel=dallas&muted=true"
    height="720"
    width="1280"
    frameborder="0"
    scrolling="no"
    allowfullscreen="true">
</iframe>

        <table>
          <thead>
            <tr>
              <th></th>
              <th></th>
              <th></th>
              <th></th>
              <th></th>
              <th></th>
            </tr>
          </thead>
          <tbody>
            <% @posts.reverse.each do |post| %>
            <tr>
              <td><%= image_tag post.user.image.url(:thumb) %></td>
              <td><%= post.user.username %></td>

              <td><%= post.subject %></td>

              <td>posted <%= time_ago_in_words(post.created_at||Time.now) %> ago.</td>

              <% if current_user && post.user.id == current_user.id %>
              <td><%= link_to 'Show', post %></td>

              <td><%= link_to 'Edit', edit_post_path(post) %></td>

              <% else %>

              <td><%= link_to 'Show', post %></td>

              <% end %>
            </tr>
            <% end %>
          </tbody>
        </table>

      </div>

      <br>
